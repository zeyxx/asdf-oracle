# Helius API (required)
HELIUS_API_KEY=your-helius-api-key

# Helius Webhook Secret (for real-time updates)
# Generate with: openssl rand -hex 32
# Must match the Authentication Header in Helius dashboard
HELIUS_WEBHOOK_SECRET=your-webhook-secret

# Token Configuration
TOKEN_MINT=9zB5wRarXMj86MymwLumSKA1Dx35zPqqKfcZtK1Spump
TOKEN_SYMBOL=asdfasdfa

# Minimum balance to be considered a holder (in raw units)
MIN_BALANCE=1000

# Server port (when running standalone)
PORT=3001

# Maintenance mode (set to 1 to show "this is fine" page)
MAINTENANCE=0

# Helius rate limit (default 50, Pro plan allows 100)
HELIUS_RATE_LIMIT=50
